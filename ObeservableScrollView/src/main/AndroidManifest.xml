<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidobservablescrollviewdemo">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".ObservableListViewActivity" />
        <activity android:name=".ObservableToolbarActivity" />
        <activity android:name=".ParallexImageActivity" />
        <activity android:name=".ParallexImageListActivity" />
        <activity android:name=".StickerHeaderScrollViewActivity">

        </activity>
        <activity android:name=".ParallexAndStickerHeaderActivity" />
        <activity android:name=".ViewPagerTabActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>